				<item Name="world" />
				<item Name="self" />
				<item Name="parent" />
				<item Name="primary weapon" />
				<item Name="secondary weapon" />
				<item Name="assassination" />
			</_field_short_enum>
			<_field_string_id name="proxy marker" />
