<plugin>
	<_field_int64_integer name="vtable space*~!" />
	<_field_struct name="internal struct" />
	<_field_long_integer name="global tag ID!" />
	<_field_pad name="local tag handle!~" length="4" />
	<_field_block_64 name="affected files" max="32">
		<_field_tag_reference_64 name="file to touch^" />
	</_field_block_64>
	<_field_block_64 name="shared variables" max="32">
		<_field_string name="name^" />
		<_field_block_64 name="touches" max="32">
			<_field_long_block_index name="file to touch" />
			<_field_long_string name="field to touch" />
		</_field_block_64>
		<_field_real name="numerical value" />
	</_field_block_64>
</plugin>