<!--This is a mockup plugin because I like the format but dealing with offsets and stuff sounds annoying atm -->
<!-- Made by Zedd lol -->
<plugin>
	<_field_int64_integer name="vtable space*~!" />
	<_field_struct name="internal struct" />
	<_field_long_integer name="global tag ID!" />
	<_field_pad name="local tag handle!~" length="4" />
	<_field_block_64 name="responses" max="10">
		<_field_byte_flags name="audience">
			<item Name="cause player" />
			<item Name="cause team" />
			<item Name="effect player" />
			<item Name="effect team" />
			<item Name="everyone" />
		</_field_byte_flags>
		<_field_byte_flags name="excluded audience">
			<item Name="cause player" />
			<item Name="cause team" />
			<item Name="effect player" />
			<item Name="effect team" />
			<item Name="everyone" />
		</_field_byte_flags>
		<_field_byte_flags name="split screen flags">
			<item Name="disable if split screen" />
			<item Name="disable if not split screen" />
			<item Name="disable if split screen on different teams" />
			<item Name="disable if split screen all on same team" />
		</_field_byte_flags>
		<_field_pad name="RMPES" length="1" />
		<_field_tag_reference_64 name="response" />
	</_field_block_64>
	<_field_pad name="pad_tp" length="4" />
</plugin>