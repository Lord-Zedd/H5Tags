	<_field_string_id name="Fallback Player Start Event." />
	<_field_string_id name="Fallback Player Stop Event." />
	<_field_tag_reference_64 name="Sound bank#Reference to the sound bank tag this event is in. Can be empty." />
	<_field_tag_reference_64 name="Player sound bank#Reference to the player sound bank tag this event is in. Can be empty." />
