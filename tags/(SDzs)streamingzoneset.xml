<!--This is a mockup plugin because I like the format but dealing with offsets and stuff sounds annoying atm -->
<!-- Made by Zedd lol -->
<plugin>
	<_field_int64_integer name="vtable space*~!" />
	<_field_struct name="internal struct" />
	<_field_long_integer name="global tag ID!" />
	<_field_pad name="local tag handle!~" length="4" />
	<_field_real_point_2d name="resource subregion AABB min" />
	<_field_real_vector_2d name="resource subregion block size" />
	<_field_block_64 name="streaming resource ids" max="4096">
		<_field_tag_reference_64 name="streaming_reference_tag" />
	</_field_block_64>
	<_field_block_64 name="streaming resource subregions" max="65536">
		<_field_long_integer name="block offset X" />
		<_field_long_integer name="block offset Y" />
		<_field_real name="min Z" />
		<_field_real name="max Z" />
		<_field_block_64 name="resource levels" max="4096">
			<_field_short_integer name="requested width" />
			<_field_short_integer name="requested height" />
		</_field_block_64>
	</_field_block_64>
	<_field_pageable_resource_64 name="resource data" />
</plugin>