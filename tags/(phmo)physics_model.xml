		<_field_string_id name="name" />
		<_field_real name="maximum force#0 defaults to k_real_max" />
		<_field_real name="minimum force#0 defaults to maximum force.  In general you should leave this alone unless working on ragdolls or something like them" />
		<_field_real name="spring k" />
		<_field_real name="damping#0 means default daming of 1.0f" />
		<_field_real name="initial position" />
	</_field_block_64>
