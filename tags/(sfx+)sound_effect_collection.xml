				<item Name="don't play at start" />
				<item Name="play on stop" />
				<item Name="play alternate" />
				<item Name="sync with origin looping sound" />
			</_field_long_flags>
		</_field_block_64>
	</_field_block_64>
	<_field_pad name="pad_tp" length="4" />
