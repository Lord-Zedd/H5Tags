			<item Name="only turn on in first person" />
		</_field_long_flags>
		<_field_tag_reference_64 name="radio effect" />
		<_field_tag_reference_64 name="Start event" validtags="snd!" />
		<_field_tag_reference_64 name="End event" validtags="snd!" />
