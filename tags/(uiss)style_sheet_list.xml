<!--This is a mockup plugin because I like the format but dealing with offsets and stuff sounds annoying atm -->
<!-- Made by Zedd lol -->
<plugin>
	<_field_int64_integer name="vtable space*~!" />
	<_field_struct name="internal struct" />
	<_field_long_integer name="global tag ID!" />
	<_field_pad name="local tag handle!~" length="4" />
	<_field_block_64 name="style sheet languages" max="17">
		<_field_long_enum name="language^">
			<item Name="english" />
			<item Name="japanese" />
			<item Name="german" />
			<item Name="french" />
			<item Name="spanish" />
			<item Name="mexican spanish" />
			<item Name="italian" />
			<item Name="korean" />
			<item Name="chinese-traditional" />
			<item Name="chinese-simplified" />
			<item Name="portuguese" />
			<item Name="polish" />
			<item Name="russian" />
			<item Name="danish" />
			<item Name="finnish" />
			<item Name="dutch" />
			<item Name="norwegian" />
		</_field_long_enum>
		<_field_block_64 name="font style sheets" max="64">
			<_field_string_id name="style sheet name^" />
			<_field_block_64 name="font id" max="1">
				<_field_long_enum name="font id">
					<item Name="terminal font" />
					<item Name="subtitle font" />
				</_field_long_enum>
			</_field_block_64>
			<_field_block_64 name="text case" max="1">
				<_field_long_enum name="text case">
					<item Name="normal" />
					<item Name="uppercase" />
					<item Name="lowercase" />
				</_field_long_enum>
			</_field_block_64>
			<_field_block_64 name="justification" max="1">
				<_field_long_enum name="justification">
					<item Name="left" />
					<item Name="right" />
					<item Name="center" />
				</_field_long_enum>
			</_field_block_64>
			<_field_block_64 name="alignment" max="1">
				<_field_long_enum name="alignment">
					<item Name="top" />
					<item Name="bottom" />
					<item Name="center" />
				</_field_long_enum>
			</_field_block_64>
			<_field_block_64 name="scale" max="1">
				<_field_real name="value" />
			</_field_block_64>
			<_field_block_64 name="fixed height" max="1">
				<_field_real name="height" />
			</_field_block_64>
		</_field_block_64>
		<_field_block_64 name="visual style sheets" max="64">
			<_field_string_id name="style sheet name^" />
			<_field_block_64 name="drop shadow style" max="1">
				<_field_long_enum name="drop shadow style">
					<item Name="none" />
					<item Name="drop" />
					<item Name="outline" />
				</_field_long_enum>
			</_field_block_64>
			<_field_block_64 name="text color" max="1">
				<_field_real_argb_color name="color" />
			</_field_block_64>
			<_field_block_64 name="drop shadow color" max="1">
				<_field_real_argb_color name="color" />
			</_field_block_64>
			<_field_string_id name="gradient name" />
			<_field_string_id name="dropshadow name" />
			<_field_string_id name="outer glow name" />
		</_field_block_64>
	</_field_block_64>
	<_field_block_64 name="gradients" max="32">
		<_field_string_id name="gradient name^" />
		<_field_real_argb_color name="color1" />
		<_field_real_argb_color name="color2" />
		<_field_real_argb_color name="color3" />
		<_field_real_argb_color name="color4" />
		<_field_real name="angle" />
		<_field_real name="scale" />
		<_field_real_vector_2d name="offset" />
		<_field_long_integer name="number of colors" />
		<_field_long_enum name="gradient shape">
			<item Name="linear" />
			<item Name="circular" />
			<item Name="diamond" />
			<item Name="square" />
		</_field_long_enum>
	</_field_block_64>
	<_field_block_64 name="dropshadows" max="32">
		<_field_string_id name="dropshadow name^" />
		<_field_real_argb_color name="color" />
		<_field_real name="angle" />
		<_field_real name="distance" />
		<_field_real name="spread" />
		<_field_real name="size" />
	</_field_block_64>
	<_field_block_64 name="outer glows" max="32">
		<_field_string_id name="outer glow name^" />
		<_field_real_argb_color name="color" />
		<_field_real name="spread" />
		<_field_real name="size" />
	</_field_block_64>
</plugin>