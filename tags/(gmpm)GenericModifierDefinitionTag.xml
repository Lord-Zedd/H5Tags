<!--This is a mockup plugin because I like the format but dealing with offsets and stuff sounds annoying atm -->
<!-- Made by Zedd lol -->
<plugin>
	<_field_int64_integer name="vtable space*~!" />
	<_field_struct name="internal struct" />
	<_field_long_integer name="global tag ID!" />
	<_field_pad name="local tag handle!~" length="4" />
	<_field_struct name="modifiers" />
	<_field_block_64 name="modifiers float" max="32">
		<_field_struct name="modifier float" />
		<_field_string_id name="property" />
		<_field_char_enum name="scope">
			<item Name="global" />
			<item Name="object" />
			<item Name="player" />
			<item Name="team" />
			<item Name="mount" />
		</_field_char_enum>
		<_field_pad name="pad0" length="3" />
		<_field_real name="duration" />
		<_field_real name="modifier" />
	</_field_block_64>
	<_field_block_64 name="modifiers int" max="32">
		<_field_struct name="modifier int" />
		<_field_string_id name="property" />
		<_field_char_enum name="scope">
			<item Name="global" />
			<item Name="object" />
			<item Name="player" />
			<item Name="team" />
			<item Name="mount" />
		</_field_char_enum>
		<_field_pad name="pad0" length="3" />
		<_field_real name="duration" />
		<_field_long_integer name="modifier" />
	</_field_block_64>
	<_field_block_64 name="modifiers bool" max="32">
		<_field_struct name="modifier bool" />
		<_field_string_id name="property" />
		<_field_char_enum name="scope">
			<item Name="global" />
			<item Name="object" />
			<item Name="player" />
			<item Name="team" />
			<item Name="mount" />
		</_field_char_enum>
		<_field_pad name="pad0" length="3" />
		<_field_real name="duration" />
		<_field_char_enum name="modifier">
			<item Name="false" />
			<item Name="true" />
		</_field_char_enum>
		<_field_pad name="pad00" length="3" />
	</_field_block_64>
	<_field_pad name="pad0" length="4" />
</plugin>