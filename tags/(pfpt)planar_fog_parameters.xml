<plugin>
	<_field_int64_integer name="vtable space*~!" />
	<_field_struct name="internal struct" />
	<_field_long_integer name="global tag ID!" />
	<_field_pad name="local tag handle!~" length="4" />
	<_field_word_flags name="flags">
		<item Name="enable patchy effect" />
		<item Name="enable color palette" />
		<item Name="enable alpha palette" />
		<item Name="render only" />
	</_field_word_flags>
	<_field_pad name="ABCDadf" length="2" />
	<_field_real name="fog thickness [0.0 to 1.0]" />
	<_field_real name="per-vertex fog thickness modulation:(only for transparents)" />
	<_field_real name="full fog depth:world units" />
	<_field_real_rgb_color name="fog color" />
	<_field_real name="fog color intensity" />
	<_field_tag_reference_64 name="palette texture" />
	<_field_custom name="patchy effect" />
	<_field_tag_reference_64 name="patchy texture" />
	<_field_real_rgb_color name="patchy color" />
	<_field_real name="patchy color intensity" />
	<_field_real name="patchy texture tile size:world units" />
	<_field_real name="patchy distance between sheets:world units" />
	<_field_real name="patchy z-buffer fade factor" />
	<_field_real name="patchy distance falloff start:world units" />
	<_field_real name="patchy distance falloff power:world units" />
	<_field_real name="patchy density" />
	<_field_real name="patchy surface depth:world units" />
	<_field_real name="patchy fade range:world units" />
	<_field_real_vector_3d name="patchy wind direction:world units" />
	<_field_custom name="LoD settings (N/A yet)" />
	<_field_real name="max fog draw distance:world units" />
	<_field_real name="patchy fade start distance:world units" />
	<_field_real name="patchy fade end distance:world units" />
</plugin>