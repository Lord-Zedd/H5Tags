<!--This is a mockup plugin because I like the format but dealing with offsets and stuff sounds annoying atm -->
<!-- Made by Zedd lol -->
<plugin>
	<_field_int64_integer name="vtable space*~!" />
	<_field_struct name="internal struct" />
	<_field_long_integer name="global tag ID!" />
	<_field_pad name="local tag handle!~" length="4" />
	<_field_string_id name="name^#NodeGraph stack name" />
	<_field_long_enum name="transform method">
		<item Name="Default 2D" />
		<item Name="Object Marker 3D" />
		<item Name="Primary Weapon Marker 3D" />
		<item Name="Primary Weapon Marker 2D" />
		<item Name="Inherited" />
		<item Name="Weapon Reticle" />
	</_field_long_enum>
	<_field_long_enum name="projection method">
		<item Name="Default 2D" />
		<item Name="Player View" />
		<item Name="Inherited" />
	</_field_long_enum>
	<_field_long_enum name="render mode">
		<item Name="2D" />
		<item Name="3D In World" />
	</_field_long_enum>
	<_field_string_id name="markerName^" />
	<_field_real name="scale" />
	<_field_long_enum name="loading mode">
		<item Name="LoadWhenActive" />
		<item Name="Preload" />
	</_field_long_enum>
	<_field_block_64 name="node graphs" max="32">
		<_field_string_id name="name^#NodeGraph name" />
		<_field_string_id name="initial node" />
		<_field_block_64 name="nodes" max="128">
			<_field_string_id name="name^#Node name" />
			<_field_string_id name="Placement Node name" />
			<_field_block_64 name="transitions" max="64">
				<_field_string_id name="event^" />
				<_field_string_id name="target node" />
				<_field_string name="out animation name" />
				<_field_string name="in animation name" />
				<_field_long_flags name="flags">
					<item Name="Override Default" />
				</_field_long_flags>
				<_field_real name="editor from angle!" />
				<_field_real name="editor to angle!" />
				<_field_real name="editor bezier from x!" />
				<_field_real name="editor bezier from y!" />
				<_field_real name="editor bezier to x!" />
				<_field_real name="editor bezier to y!" />
				<_field_real name="editor caption x!" />
				<_field_real name="editor caption y!" />
			</_field_block_64>
			<_field_long_enum name="screen wrapper">
				<item Name="guess from screen tag" />
				<item Name="none" />
				<item Name="WPF" />
				<item Name="CUI" />
				<item Name="NodeGraph" />
				<item Name="SharedNodeGraph" />
				<item Name="ADSWeaponNode" />
				<item Name="VehicleNode" />
				<item Name="FUI" />
			</_field_long_enum>
			<_field_tag_reference_64 name="screen reference" validtags="cusc, wpfs, ngst, uiwn, uivn, fuis" />
			<_field_tag_reference_64 name="fui screen reference" />
			<_field_long_enum name="loading mode">
				<item Name="LoadWhenActive" />
				<item Name="Preload" />
			</_field_long_enum>
			<_field_block_64 name="child stacks" max="32">
				<_field_tag_reference_64 name="child stack reference" />
			</_field_block_64>
			<_field_long_flags name="state flags">
				<item Name="Allow Input Fall Through" />
				<item Name="Matchmaking status should appear on top of this" />
				<item Name="Hide On Spartan Render Mode (When in that mode, hide this screen)" />
				<item Name="Allow Input Fall Through Even When Handled" />
				<item Name="Draw on top of fades" />
				<item Name="Allow Gameplay Input" />
			</_field_long_flags>
			<_field_real name="editor pos x!" />
			<_field_real name="editor pos y!" />
			<_field_long_flags name="editor flags!">
				<item Name="locked" />
			</_field_long_flags>
			<_field_long_enum name="editor shape!">
				<item Name="rectangle" />
				<item Name="hexagon" />
				<item Name="trapezoid" />
				<item Name="circle" />
				<item Name="diamond" />
			</_field_long_enum>
		</_field_block_64>
	</_field_block_64>
</plugin>