<!--This is a mockup plugin because I like the format but dealing with offsets and stuff sounds annoying atm -->
<!-- Made by Zedd lol -->
<plugin>
	<_field_int64_integer name="vtable space*~!" />
	<_field_struct name="internal struct" />
	<_field_long_integer name="global tag ID!" />
	<_field_pad name="local tag handle!~" length="4" />
	<_field_struct name="Track" />
	<_field_byte_flags name="Flags">
		<item Name="Lens Enabled" />
	</_field_byte_flags>
	<_field_pad name="PAD1!" length="3" />
	<_field_block_64 name="Control Points" max="2000">
		<_field_real_vector_3d name="position" />
		<_field_real_quaternion name="orientation" />
	</_field_block_64>
	<_field_block_64 name="Lenses" max="2000">
		<_field_byte_flags name="Flags">
			<item Name="Depth Of Field Enabled" />
		</_field_byte_flags>
		<_field_pad name="PAD1!" length="3" />
		<_field_real name="Focal Length" />
		<_field_real name="Focus Distance" />
		<_field_real name="FStop" />
	</_field_block_64>
	<_field_pad name="EXPLICIT_PAD!" length="4" />
</plugin>