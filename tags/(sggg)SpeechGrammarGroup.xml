		<_field_struct name="Script Action" />
		<_field_long_integer name="script name!~" />
		<_field_long_string name="script source" />
		<_field_explanation name="Script Condition" />
		<_field_struct name="Script Condition" />
		<_field_long_integer name="script name!~" />
		<_field_long_string name="script source" />
