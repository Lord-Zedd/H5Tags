<!--This is a mockup plugin because I like the format but dealing with offsets and stuff sounds annoying atm -->
<!-- Made by Zedd lol -->
<plugin>
	<_field_int64_integer name="vtable space*~!" />
	<_field_struct name="internal struct" />
	<_field_long_integer name="global tag ID!" />
	<_field_pad name="local tag handle!~" length="4" />
	<_field_block_64 name="template collection" max="8">
		<_field_string_id name="dsp effect" />
		<_field_explanation name="WARNING">DON'T MODIFY THIS TAG UNLESS YOU KNOW WHAT YOU ARE DOING</_field_explanation>
		<_field_data_64 name="explanation" />
		<_field_long_flags name="flags">
			<item Name="use high level parameters" />
			<item Name="custom parameters" />
		</_field_long_flags>
		<_field_short_integer name="dsp state offset!" />
		<_field_short_integer name="dsp state size!" />
		<_field_block_64 name="parameters" max="128">
			<_field_string_id name="name" />
			<_field_short_enum name="type">
				<item Name="integer" />
				<item Name="real" />
				<item Name="filter type" />
			</_field_short_enum>
			<_field_word_flags name="flags">
				<item Name="expose as function" />
			</_field_word_flags>
			<_field_long_integer name="hardware offset" />
			<_field_long_integer name="default enum integer value" />
			<_field_real name="default scalar value" />
			<_field_struct name="default function" />
			<_field_data_64 name="data" />
			<_field_real name="minimum scalar value" />
			<_field_real name="maximum scalar value" />
		</_field_block_64>
	</_field_block_64>
	<_field_string_id name="internal dsp effect name{input effect name}" />
	<_field_block_64 name="additional sound inputs" max="1">
		<_field_string_id name="dsp effect" />
		<_field_struct name="low frequency sound" />
		<_field_data_64 name="data" />
		<_field_real name="time period: seconds" />
	</_field_block_64>
	<_field_pad name="pad_set_0" length="4" />
</plugin>